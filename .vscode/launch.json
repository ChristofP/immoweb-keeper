    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run immoweb search",
            "type": "node",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "runtimeExecutable": "${env:NVM_BIN}/npm", //change this to your npm path
            "runtimeArgs": [
                "run-script",
                "start",
                "--",
                "--inspect-brk=9229"
            ],
            "port": 9229
            }
    ]
}